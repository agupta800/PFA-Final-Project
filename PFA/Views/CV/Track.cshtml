@model FileModel



<h2>Track File Status</h2>

<form asp-controller="CV" asp-action="GetStatus" method="post">
    <div class="form-group">
        <label for="trackingId">Enter Tracking ID:</label>
        <input type="text" id="trackingId" name="trackingId" class="form-control" required />
    </div>
    <button type="submit" class="btn btn-primary">Track Status</button>
</form>
